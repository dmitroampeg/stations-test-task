{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "assets/*": ["src/assets/*"],
      "components/*": ["src/components/*"],
      "features/*": ["src/features/*"],
      "hooks/*": ["src/hooks/*"],
      "icons/*": ["src/icons/*"],
      "types/*": ["src/types/*"],
      "utils/*": ["src/utils/*"],
      "mocks/*": ["src/mocks/*"],
      "helpers/*": ["src/helpers/*"]
    }
  }
}
